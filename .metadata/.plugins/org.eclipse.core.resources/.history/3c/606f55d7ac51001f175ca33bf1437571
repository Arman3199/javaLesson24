package day6;

public class Largest_LowestNum_inArray {

	public static void main(String[] args) {
		//q4) find largest&lowest numbers of array
		
		int arr[]= {3,8,2,67,43,34};
		
		int min=arr[0];
		int max=arr[0];
		
		for(int i=0; i<arr.length; i++) {
			if(arr[i] <min) {
				min=arr[i];
			} else if (arr[i]>max) {
				max=arr[i];
			}
		}
		System.out.println("Min number is is: " +min);
		System.out.println("Max number is is: " +max);
	}



}
